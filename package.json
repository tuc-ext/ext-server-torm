{
  "name": "ubi.server.torm",
  "version": "0.1.0",
  "private": true,
  "author": "",
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "boot": "npm i run-script-os && run-script-os",
    "boot:nix": "rm -fr node_modules && rm -fr package-lock.json && npm i",
    "boot:windows": "rd /s /q node_modules && rd /s /q package-lock.json && npm i",
    "ssl": "certbot certonly -d server.ubi.yuanjin.net --standalone",
    "debug": "cross-env NODE_ENV=development node --inspect-brk server.js",
    "debug.sup": "cross-env NODE_ENV=development supervisor -i node_modules,logbook,database --inspect-brk -- server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "dev.sup": "cross-env NODE_ENV=development supervisor -i node_modules,logbook,database -- server.js",
    "dev.pm2": "cross-env NODE_ENV=development pm2 start server.js --name ubi.server.torm",
    "drive": "cross-env NODE_ENV=production nodemon server.js",
    "drive.sup": "cross-env NODE_ENV=production supervisor -i node_modules,logbook,database -- server.js",
    "drive.pm2": "cross-env NODE_ENV=production pm2 start server.js --name ubi.server.torm",
    "deploy": "expect deploy-expect.sh server.ubi.yuanjin.net"
  },
  "dependencies": {
    "body-parser": "^1.16.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "errorhandler": "^1.5.0",
    "etherscan-api": "^10.0.5",
    "express": "^4.14.1",
    "ipfs-core": "^0.5.4",
    "method-override": "^2.3.7",
    "morgan": "^1.8.1",
    "multer": "^1.4.2",
    "node-schedule": "^1.3.0",
    "serve-favicon": "^2.5.0",
    "sol.basetool": "git+https://git.faronear.org/npm/sol.basetool",
    "sol.enviconfig": "git+https://git.faronear.org/npm/sol.enviconfig",
    "sol.i18n": "git+https://git.faronear.org/npm/sol.i18n",
    "sol.logger": "git+https://git.faronear.org/npm/sol.logger",
    "sol.messenger": "git+https://git.faronear.org/npm/sol.messenger",
    "sol.nettool": "git+https://git.faronear.org/npm/sol.nettool",
    "sol.webServerSocket": "git+https://git.faronear.org/npm/sol.webServerSocket",
    "sol.webtoken": "git+https://git.faronear.org/npm/sol.webtoken",
    "sqlite3": "^5.0.0",
    "tic.crypto": "git+https://git.faronear.org/npm/tic.crypto",
    "typeorm": "^0.2.24",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.2",
    "pm2": "^4.5.0",
    "run-script-os": "^1.1.6",
    "standardx": "^6.0.0",
    "supervisor": "^0.12.0"
  }
}
